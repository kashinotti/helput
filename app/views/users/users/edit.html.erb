<h1>プロフィール編集</h1>

<div>
  <%= form_with model: @user, url: user_path(@user.id), method: :patch, local: true do |f| %>
  
    <div>
      <%= f.text_field :name %>
    </div>
    
    <div>
      <%= f.attachment_field :profile_image %>
    </div>
    
    <div>
      <%= f.text_area :introduce %>
    </div>
    
    <div>
      <%= f.submit "変更を保存する" %>
    </div>
    
  <% end %>
</div>